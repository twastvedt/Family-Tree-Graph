{
  "name": "family-tree-graph",
  "version": "0.1.0",
  "description": "A D3 family tree graph generated from a GRAMPS XML export",
  "author": "Trygve Wastvedt <trygve.wastvedt@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "run-p vite type-check",
    "vite": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc -w --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "d3": "7.x",
    "luxon": "^3.4.4",
    "mousetrap": "^1.6.5",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/d3": "7.x",
    "@types/d3-drag": "^3.0.7",
    "@types/luxon": "^3.3.5",
    "@types/mousetrap": "^1.6.15",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vue-tsc": "^1.8.19"
  }
}
